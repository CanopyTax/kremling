{
  "coveragePathIgnorePatterns": ["<rootDir>/node_modules"],
  "coverageReporters": [
    "lcov",
    "text-summary",
    "text"
  ],
  "testEnvironment": "jsdom",
  "collectCoverageFrom": ["src/**/*.js"],
  "transform": {
    ".*": "babel-jest"
  },
  "rootDir": "./",
  "modulePaths": ["./"],
  "setupFilesAfterEnv": ["<rootDir>/test-setup.js"]
}
